{
  "name": "gatsby-starter-gitlab",
  "private": true,
  "description": "A Gatsby starter to generate a website from a list of Gitlab groups and/or projects.",
  "version": "0.1.0",
  "author": "T-Mobile",
  "scripts": {
    "build": "npm run build:plugins && (gatsby build || (npm run clean && gatsby build))",
    "build:plugins": "cd plugins/ && sh build.sh && cd ../",
    "clean": "gatsby clean",
    "clean:start": "npm run start:clean",
    "develop": "npm run build:plugins && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "preinstall": "cd plugins && sh install.sh && cd ../",
    "serve": "gatsby serve",
    "start": "npm run develop || (npm run clean && npm run develop)",
    "start:clean": "npm run clean && npm start",
    "test": "jest --verbose=false",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "bootstrap": "^4.4.1",
    "gatsby": "^2.20.3",
    "gatsby-plugin-typegen": "^2.0.0",
    "gatsby-remark-graph": "^0.2.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-icons": "^3.0.0",
    "@types/shelljs": "^0.8.7",
    "babel-jest": "^25.2.6",
    "babel-preset-gatsby": "^0.3.1",
    "fs-extra": "^9.0.0",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-codegen": "^1.0.14",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.2",
    "gatsby-plugin-typescript": "^2.2.2",
    "gatsby-remark-relative-links": "file:plugins/gatsby-remark-relative-links",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-gitlab": "file:plugins/gatsby-source-gitlab",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "license": "Apache 2",
  "repository": {
    "type": "git",
    "url": "https://github.com/tmobile/gatsby-starter-gitlab"
  },
  "bugs": {
    "url": "https://github.com/tmobile/gatsby-starter-gitlab/issues"
  },
  "homepage": "https://github.com/tmobile/gatsby-starter-gitlab"
}
